{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "required": [
        "userId",
        "category",
        "difficulty"
    ],
    "properties": {
        "userId": {
            "type": "integer"
        },
        "category": {
            "type": "string",
            "minLength": 1,
            "maxLength": 50
        },
        "difficulty": {
            "type": "string",
            "minLength": 1,
            "maxLength": 20
        }
    }
}