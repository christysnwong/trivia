{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "required": [
        "userId",
        "question",
        "answer",
        "folderName"
    ],
    "additionalProperties": false,
    "properties": {
        "userId": {
            "type": "integer"
        },
        "question": {
            "type": "string"
        },
        "answer": {
            "type": "string"
        },
        "folderName": {
            "type": "string",
            "minLength": 1,
            "maxLength": 20
        }
    }
}